{
    "extends": "airbnb",
    "env": {
        "es6": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "indent": [
            "error",
            4
        ],
        "object-curly-spacing": [
            "error",
            "never"
        ]
    }
}